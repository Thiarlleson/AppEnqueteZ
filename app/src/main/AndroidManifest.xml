<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thiarlleson.enquentez">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.TelaInicial">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity.TelaLogin" />
        <activity android:name=".Activity.TelaCriarEnquente" />
        <activity android:name=".Activity.TelaPersonalizarEnquente" />
        <activity android:name=".Activity.TelaPrincipal" />
        <activity android:name=".Activity.TelaCadastro"></activity>
    </application>

</manifest>